<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.testingframework.selenium.dto.UseCaseClsDTO" table="ut_usecase_cls" lazy="false">
		<id name="clsName">
			<generator class="assigned"></generator>
		</id>

		<property name="ucid"></property>
		<property name="useCaseName"></property>
		<property name="modifiedBy"></property>
		<property name="modifiedDt"></property>
	</class>

</hibernate-mapping>