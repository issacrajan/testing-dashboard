<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.testingframework.selenium.dto.UserUseCaseDTO" table="ut_user_usecase" lazy="false">
		<composite-id>
			<key-property name="userId" />
			<key-property name="ucid" />
		</composite-id>

		<property name="createdDt"></property>
	</class>

</hibernate-mapping>