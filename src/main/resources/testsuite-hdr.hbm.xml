<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.testingframework.selenium.dto.TestSuiteHdrDTO" table="ut_testsuitehdr" lazy="false">
		<id name="id" type="long" unsaved-value="null">
			<generator class="sequence">
				<param name="sequence">testsuite_seq</param>
			</generator>
		</id>

		<property name="tsName"></property>
		<property name="tsDesc"></property>
		<property name="createdBy"></property>
		<property name="createdDt"></property>
	</class>

</hibernate-mapping>