<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.testingframework.selenium.dto.UseCaseDTO" table="ut_usecase"  lazy="false">
		<id name="id" type="long" unsaved-value="null">
			<generator class="sequence">
				<param name="sequence">use_case_seq</param>
			</generator>
		</id>

		<property name="moduleName"></property>
		<property name="useCaseName"></property>
		<property name="remarks"></property>
		<property name="modifiedBy"></property>
		<property name="modifiedDt"></property>
	</class>

</hibernate-mapping>